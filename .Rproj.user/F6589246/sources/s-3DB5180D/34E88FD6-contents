```{r differences between the 4 study arms}
library(gtsummary)
df_video_intervention %>%
  filter(vaccination_status==0) %>%
  select(vaccine_hesitancy, QQ) %>%
  tbl_summary(
    statistic = all_continuous() ~ "{mean} ({sd})",
    by = QQ
  ) %>%
  add_p()

df_video_intervention %>%
  select(vaccination_status, QQ) %>%
  tbl_summary(
    statistic = all_continuous() ~ "{mean} ({sd})",
    by = QQ
  ) %>%
  add_p()  
